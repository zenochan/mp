<view class='nav-wrap' style='height: {{height}}px;'>
  <view class='nav-title' style='line-height: {{height}}px;'>{{navbarData.title}}</view>
  <view style='display: flex; justify-content: space-around;flex-direction: column'>
    <view class='nav-capsule' style='height: {{height}}px;' wx:if='{{navbarData.showCapsule}}'>
      <view bindtap='_navback'>
        1
        <image src='/imgs/back-pre.png' style="background: orange" mode='aspectFill' class='back-pre'></image>
        2
      </view>
      <view class='navbar-v-line' wx:if='{{!share}}'></view>
      <view bindtap='_backhome'>
        <image src='/imgs/back-home.png' mode='aspectFill' class='back-home'></image>
      </view>
    </view>
  </view>
</view>
